program
start
    number x;
    number y;
    for x := 0 until x = 2 step x := x + 1 do
        if x = 0 then
            repeat
        fi
        for y := 0 until y = 100 step y := y + 1 do
            x! y! "\n"!
            if y = 1 then
                stop
            fi
        done
    done
end
